{
    "include": [
        "src/backend/*"
    ],
    "exclude": [
        "node_modules/",
    ],
    "compilerOptions": {
        "outDir": "dist",
        "sourceMap": true,
        "strict": true,
        "module": "CommonJS",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strictPropertyInitialization": false
    }
}